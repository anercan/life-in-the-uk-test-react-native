{
  "expo": {
    "name": "life-in-the-uk",
    "slug": "life-in-the-uk",
    "description": "Life In the UK 2024",
    "primaryColor": "#eabc7c",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#eabc7c"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.quizmarkt.lifeintheuk",
      "adaptiveIcon": {
        "backgroundColor": "#b2f6f6"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-localization",
      "@react-native-google-signin/google-signin",
      "react-native-iap"
    ],
    "extra": {
      "eas": {
        "projectId": "bb230948-a417-4a49-b054-7a2153be174e"
      }
    }
  }
}
